<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>com.bird</groupId>
    <artifactId>bird-project</artifactId>
    <packaging>pom</packaging>
    <version>1.0.0</version>

    <modules>
        <module>bird-core</module>
        <module>bird-service-common</module>
        <module>bird-service-zero</module>
        <module>bird-web-admin</module>
        <module>bird-web-api</module>
        <module>bird-service-cms</module>
        <module>bird-service-scheduler</module>
        <module>bird-eventbus</module>
        <module>bird-eventbus-kafka</module>
        <module>bird-web-sso</module>
        <module>bird-web-common</module>
    </modules>

    <properties>
        <basedir>src/main</basedir>

        <!-- 开发阶段配置文件位置-->
        <system.config.path>classpath:system.properties</system.config.path>

        <!-- 编码 -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>

        <!--spring版本号-->
        <spring.version>5.0.4.RELEASE</spring.version>
        <spring-data-redis.version>2.0.5.RELEASE</spring-data-redis.version>

        <!--junit版本号-->
        <junit.version>3.8.1</junit.version>

        <!-- 日志组件版本号 -->
        <slf4j.version>1.7.25</slf4j.version>
        <logback.version>1.1.11</logback.version>

        <!--quartz组件版本号-->
        <quartz.version>2.2.3</quartz.version>

        <!--redis组件版本号-->
        <jedis.version>2.9.0</jedis.version>

        <!--dubbo版本号-->
        <dubbo.version>2.6.0</dubbo.version>
        <zkclient.version>0.10</zkclient.version>
        <javassist.version>3.21.0-GA</javassist.version>

        <!--kafka版本号-->
        <spring-kafka.version>2.1.0.RELEASE</spring-kafka.version>

        <!--mybatis版本号-->
        <mybatis.version>3.4.4</mybatis.version>
        <mybatis-spring.version>1.3.1</mybatis-spring.version>
        <mybatis-plus.version>2.1.9</mybatis-plus.version>

        <!--mssql-jdbc版本号-->
        <mysql.version>5.1.44</mysql.version>

        <!--druid版本号-->
        <druid.version>1.0.29</druid.version>

        <!--json组件版本号-->
        <jackson.version>2.9.0.pr3</jackson.version>
        <fastjson.version>1.2.32</fastjson.version>

        <!--aop组件版本号-->
        <aspectj.version>1.8.10</aspectj.version>

        <!--反射组件版本号-->
        <reflectasm.version>1.11.4-2</reflectasm.version>

        <springfox.version>2.6.1</springfox.version>

        <!--bean映射组件版本号-->
        <dozer.version>5.5.1</dozer.version>

        <!--httpclient组件版本号-->
        <httpclient.version>4.0.2</httpclient.version>

        <!--maven插件版本号-->
        <maven-compiler-plugin.version>3.7.0</maven-compiler-plugin.version>

        <!--其他版本号-->
        <javax.version>3.1.0</javax.version>
        <jstl.version>1.2</jstl.version>
        <commons-lang3.version>3.5</commons-lang3.version>
        <commons-io.version>2.6</commons-io.version>
        <ant.version>1.10.1</ant.version>
        <commons-beanutils.version>1.9.3</commons-beanutils.version>
        <commons-httpclient.version>3.1</commons-httpclient.version>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>${maven-compiler-plugin.version}</version>
                <configuration>
                    <source>1.8</source>
                    <target>1.8</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>